<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:media2="using:Microsoft.UI.Xaml.Media"
>

    <SolidColorBrush x:Key="MainNavBackground" Color="#F1F3F6" />
    <SolidColorBrush x:Key="MainNavSelectedBackground" Color="#DEE3EE" />
    <SolidColorBrush x:Key="MainNavForeground" Color="#A6A8B5" />
    <SolidColorBrush x:Key="MainNavSelectedForeground" Color="#5843BE" />

    <SolidColorBrush x:Key="MainPlayBackground" Color="#6982F9" />
    <SolidColorBrush x:Key="MainPlayBackgroundPointOver" Color="#576CCF" />
    <SolidColorBrush x:Key="MainPlayBackgroundPressed" Color="#576CCF" />

    <media2:AcrylicBrush x:Key="PopupBackground" TintOpacity="0.8" TintLuminosityOpacity="0.8" TintColor="LightGray" FallbackColor="SkyBlue" />

</ResourceDictionary>
